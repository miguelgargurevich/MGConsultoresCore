<script>
    var host = document.location.protocol + '//' + location.host + '/';

    function enviarComentario() {
        var nombre = document.getElementsByName("name")[0].value;
        var correo = document.getElementsByName("email")[0].value;
        var mensaje = document.getElementsByName("message")[0].value;

        if (mensaje == "") {
            alert("Ingrese un comentario");
            return false;
        }
        var dataObject = JSON.stringify({
            'Nombre': nombre,
            'Correo': correo,
            'Mensaje': mensaje,
        });

        console.log(host + 'Home/EnviarComentario/');

        $.ajax({
            url: host + 'Home/EnviarComentario/',
            data: dataObject,
            contentType: 'application/json',
            type: 'POST',
            success: function (data) {
                if (data == "true") {
                    document.getElementById('contactoSuccess').style.display = "block";
                    //var myurl = '/Home/Index/';
                    //window.location.href = myurl;
                }
                else {
                    document.getElementById('contactoError').style.display = "block";
                }
            }
        });

    }

</script>